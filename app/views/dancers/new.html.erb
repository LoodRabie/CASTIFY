<%= simple_form_for @dancer do |f| %>
  <%= f.input :name %>
  <%= f.input :age %>
  <%= f.input :location %>
  <%= f.input :bio %>
  <%= f.input :profile_picture, as: :file, label: 'Profile Picture' %>
  <%= f.hidden_field :user_id, value: current_user.id %>

  <%= f.button :submit, 'Create Dancer Profile' %>
<% end %>
